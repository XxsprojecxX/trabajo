{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
       "@/lib/*": ["app/lib/*"],
      "@/components/*": ["app/components/*"]
    },
    "jsx": "react-jsx"
  },
  "include": ["app/**/*"],
  "exclude": ["node_modules", "dist"]
}